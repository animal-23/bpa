pragma solidity ^0.8.0;
contract Arthematic {
    uint16 public a=81;
    uint16 public b=9;
    uint public sum=a+b;
    uint public diff=a-b;
    uint public prod=a*b;
    uint public div=a/b;
    uint public mod=a%b;
    uint public inc=++a;
    uint public dec=--b;
}
